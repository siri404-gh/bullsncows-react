		"build:mobile:ios": "react-native run-ios",
		"start:mobile": "node node_modules/react-native/local-cli/cli.js start",
		"test:mobile": "jest",
		"start:dev:web": "./node_modules/.bin/webpack-dev-server --env=dev --progress --profile --colors --open",
