	"private": true,
	"scripts": {
		"bundle:mobile": "./node_modules/.bin/rimraf android/app/src/main/assets && mkdir android/app/src/main/assets && react-native bundle --platform android --dev false --entry-file index.android.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res/ && ./node_modules/.bin/rimraf android/app/src/main/assets && ./node_modules/.bin/rimraf .prod",
