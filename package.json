		"start:prod:web": "node server",
		"build:prod:web": "./node_modules/.bin/webpack --env=prod --progress --profile --colors",
		"test:web": "karma start",
		"commit": "git-cz",
		"heroku": "yarn build:prod:web && yarn start:prod:web",
		"travis": "yarn test:web && yarn test:mobile && yarn build:prod:web && yarn bundle:mobile"
